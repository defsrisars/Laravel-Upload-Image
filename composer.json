{
  "name": "ariby/laravel-image-upload",
  "description": "Laravel 圖片上傳套件",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Ariby",
      "email": "defsrisars@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.2",
    "illuminate/database": "~5.6",
    "illuminate/support": "~5.6",
    "paragonie/random_compat": "2.*",
    "intervention/image": "^2.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.1.4",
    "orchestra/testbench": "~3.6",
    "Orchestra/Database": "~3.6",
    "mockery/mockery": "^1.0",
    "ariby/ulid": "^0.2.1",
    "liaosankai/laravel-eloquent-i18n": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "Ariby\\LaravelImageUpload\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ariby\\tests\\": "tests/"
    },
    "classmap": [
      "tests/"
    ]
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "Ariby\\LaravelImageUpload\\LaravelImageUploadServiceProvider"
      ],
      "aliases": {
      }
    }
  }
}
